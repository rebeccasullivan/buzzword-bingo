<div>
	<div id="bingoCard">
		<table class="table table-bordered">
			{{#grouped_each 5 squares}}
			    <tr>
			    {{#each this}}
			    	<td class="bingoSquare" onclick="addMark(this)">
						<div>{{square_text}}</div>
						<img class="checkmark" src="/checkmark.png" />
					</td>
			    {{/each}}
			    </tr>
			{{/grouped_each}}
		</table>
		
		<!-- Generate New Card button with AJAX call...
		<button id="btnRefresh" type="button" class="btn btn-success" onclick="refreshCard()">Generate New Card</button>
		-->
	</div>
</div>